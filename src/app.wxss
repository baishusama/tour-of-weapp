.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.has-reached-end-info {
  border-top: 1px solid #dedede;
  padding: 40rpx;
  color: #aaa;
  background-color: #e5e5e5;
  font-size: 36rpx;
  text-align: center;
}
